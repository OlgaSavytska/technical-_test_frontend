{"ast":null,"code":"import QuizHeader from './QuizHeader.vue';\nimport QuizBody from './QuizBody.vue';\nexport default {\n  name: \"QuizPage\",\n  components: {\n    QuizHeader,\n    QuizBody\n  },\n  data() {\n    return {\n      openQuiz: false\n    };\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.scrollCart);\n    this.scrollCart();\n    setTimeout(() => {\n      this.openQuiz = true;\n    }, 2000);\n  },\n  beforeDestroy() {\n    window.removeEventListener(\"scroll\", this.scrollCart);\n  },\n  methods: {\n    scrollCart() {\n      let mountains_back = document.getElementById(\"mountains_back\");\n      let mountains_front = document.getElementById(\"mountains_front\");\n      let start_title = document.getElementById(\"start_title\");\n      let value = window.scrollY;\n      let titleValue = window.scrollY + 200;\n      mountains_back.style.top = value * 0.5 + \"px\";\n      start_title.style.top = titleValue * 1 + \"px\";\n      mountains_front.style.top = value * 0 + \"px\";\n    }\n  }\n};","map":{"version":3,"names":["QuizHeader","QuizBody","name","components","data","openQuiz","mounted","window","addEventListener","scrollCart","setTimeout","beforeDestroy","removeEventListener","methods","mountains_back","document","getElementById","mountains_front","start_title","value","scrollY","titleValue","style","top"],"sources":["src/components/QuizPageComponents/QuizPage.vue"],"sourcesContent":["<template>\n    <div>\n        <v-expand-x-transition>\n            <div class=\"page\" v-show=\"openQuiz\">\n                <div class=\"container\" >\n                    <QuizHeader/>\n\n                    <div class=\"image\" >\n                        <div class=\"start_title\" id=\"start_title\">Start!</div>\n                        <div class=\"stars\" id=\"stars\"></div>\n                        <div class=\"mountains_back\" id=\"mountains_back\"></div>\n                        <div class=\"mountains_front\" id=\"mountains_front\"></div>\n                    </div>\n                    <QuizBody/>\n                </div>\n            </div>\n        </v-expand-x-transition>\n    </div>\n</template>\n\n<script>\nimport QuizHeader from './QuizHeader.vue'\nimport QuizBody from './QuizBody.vue'\n\nexport default{\n    name: \"QuizPage\",\n\n    components: {\n        QuizHeader,\n        QuizBody\n    },\n\n    data(){\n        return {\n            openQuiz: false\n        }\n    },\n\n    mounted(){\n        window.addEventListener(\"scroll\", this.scrollCart)\n        this.scrollCart()\n        \n        setTimeout(() => {\n            this.openQuiz = true\n        }, 2000)\n    },\n\n    beforeDestroy(){\n        window.removeEventListener(\"scroll\", this.scrollCart)\n    },\n\n    methods: {\n        scrollCart(){\n            let mountains_back = document.getElementById(\"mountains_back\")\n            let mountains_front = document.getElementById(\"mountains_front\")\n            let start_title = document.getElementById(\"start_title\");\n\n            let value = window.scrollY\n            let titleValue = window.scrollY + 200\n\n            mountains_back.style.top = value * 0.5 + \"px\";\n            start_title.style.top = titleValue * 1 + \"px\";\n            mountains_front.style.top = value * 0 + \"px\";\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n    .page{\n        position: absolute;\n        top: 18%;\n        z-index: 10;\n        background: rgba(255,255,255, 0.5);\n        left: 50%;\n        transform: translateX(-50%);\n        width: 100%;\n        padding: 10px 25px;\n\n        .container{\n            background: rgba(255,255,255, 1); \n            padding: 10px 25px;\n        }\n    }\n\n    .image{\n        height: 500px;\n        border: 1px red solid;\n        overflow: hidden;\n        perspective: 1000px; \n        background-color: #03031e;\n        position: relative;\n        margin-bottom: 20px;\n\n        .stars{\n            background: url('https://raw.githubusercontent.com/Ryoley/moonlight-parallax/main/assets/images/stars.png');\n            background-size: cover;\n            height: 100%;            \n        }\n\n        .mountains_back{\n            background: url('https://raw.githubusercontent.com/Ryoley/moonlight-parallax/main/assets/images/mountains_back.png');\n            background-size: cover;\n            height: 100%;\n        }\n\n        .mountains_front{\n            background: url('https://raw.githubusercontent.com/Ryoley/moonlight-parallax/main/assets/images/mountains_front.png');\n            background-size: cover;\n            height: 100%;\n        }\n\n        .stars,\n        .mountains_back,\n        .mountains_front {\n            width: 100%;\n            height: 100%;\n            background-size: cover;\n            background-repeat: no-repeat;\n            position: absolute;\n            // top: 50%;\n            // left: 50%;\n            // transform: translate(-50%, -50%);\n        }\n\n        // &:hover{\n        //     .forest,\n        //     .forest-light,\n        //     .mountains_back,\n        //     .mountains_front {\n        //         transform: translate(-50%, -50%) scale(1.2);\n        //     }\n        // }\n    }\n\n    .start_title{\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        transform: translate(-50%, -50%);\n        width: 100%;\n        padding: 30px 100px;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        z-index: 10000;\n        color: white;\n        font-size: 130px;\n        font-weight: 600;\n    }\n\n    @media(max-width: 525px){\n        .start_title{\n            font-size: 90px;\n            font-weight: 600;  \n        }\n    }\n\n    @media(max-width: 380px){\n        .page{\n            top: 30%;\n        }\n    }\n</style>"],"mappings":"AAqBA,OAAAA,UAAA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EAEAC,UAAA;IACAH,UAAA;IACAC;EACA;EAEAG,KAAA;IACA;MACAC,QAAA;IACA;EACA;E